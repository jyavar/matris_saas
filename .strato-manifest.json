{
  "root": "./",
  "validPaths": [
    "apps/frontend/**",
    "apps/backend/**",
    "apps/web/**",
    "packages/**",
    "scripts/**",
    "agent/**",
    "modules/**",
    "audit-artifacts/**",
    "public/**",
    "types/**",
    ".github/**",
    ".husky/**",
    "package.json",
    "pnpm-lock.yaml",
    ".strato-manifest.json",
    "tsconfig.base.json",
    "*.md",
    "tsconfig.json",
    "*.sh",
    "apps/**/.eslintignore",
    "packages/**/.eslintignore",
    "supabase/**",
    ".gitignore",
    "matrix.audit.json",
    "matrix.coverage.json",
    "matrix.logic.json",
    "matrix.report.json",
    "matrix.verified.json",
    "audit.extract-audit.auditable.json",
    "audit.verify-coverage.auditable.json",
    "audit.verify-exists.auditable.json",
    "audit.verify-logic.auditable.json",
    "logic.overrides.json",
    "validate-md-checklist.ts"
